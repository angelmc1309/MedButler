<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="Privacy &amp; Security">
        <Preference
            android:title="Privacy Policy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:clickable="true"
            android:summary="See Privacy Policy of MedButler"
            android:key="key_privacy_policy"/>

        <Preference
            android:title="Terms &amp; Conditions"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:clickable="true"
            android:summary="See Terms &amp; Conditions of MedButler"
            android:key="key_terms_conditions"/>

    </PreferenceCategory>
</PreferenceScreen>